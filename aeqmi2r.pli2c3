  /* CALL ANALYZE_LOCAL_UIB;

  /* IF SYSTEM_LIMITED
  /*   THEN DO;
  /*     ET_TEXT2 = ERR2||' DL/I STATUS = ' || Q1_STAT;
  /*     CALL WRITE_ERROR_MESSAGES;
  /*   END;   */

 END INSERT_SEGMENTS;


 /*********************************************************************/
 /*                                                                   */
 /*********************************************************************/
 TERMINATE_PSB: PROC;
   FUNC    = 'TERM';                     /* TERMINATE AEQMI2R PSB     */
   PSBNAME = 'AEQMI2R ';

   ET_TEXT1 = ERR2||MODULE||' UNABLE TO TERMINATE '||PSBNAME;

   IF TEST_SW
     THEN DO;
        DIAG_TEXT = MODULE||' ABOUT TO '||FUNC||' '|| PSBNAME;
        CALL WRITE_TO_OPER;
     END;
   CALL PLITDLI (THREE,FUNC,PSBNAME,UIBPTR);
   CALL ANALYZE_LOCAL_UIB;

   IF SYSTEM_LIMITED
     THEN DO;
       ET_TEXT2 = ERR2||' DL/I STATUS = ' || Q1_STAT;
       CALL WRITE_ERROR_MESSAGES;
     END;


 END TERMINATE_PSB;

 %PAGE;
 DELETE_INS_QSGENL: PROC REORDER;
 /*********************************************************************/
 /*  TRIED TO INSERT A DUPLICATE QSAR, DELETE THE QSAR ROOT           */
 /*  AT THE HUB QSAR DATA BASE AND INSERT THE NEW QSGENL SEGMENT      */
 /*********************************************************************/
   IF TEST_SW
     THEN DO;
       DIAG_TEXT = MODULE||' DELETE INSERT PROC ';
       CALL WRITE_TO_OPER;
     END;

   FUNC = 'GHU ';
       IF TEST_SW
         THEN DO;
           DIAG_TEXT = MODULE||' ABOUT TO '||FUNC||' GENL SEGMENTS';
           CALL WRITE_TO_OPER;
         END;

   ET_TEXT1 = ERR2||MODULE||' UNABLE TO '||FUNC||' QSGENL SEGMENT '||
                                     'AT THE HUB QSAR DB ';
   CALL PLITDLI (FOUR,
                 FUNC,
                 QSAR1_PCB,
                 IO_BUF,
                 SSA_QSGENL_QUAL);

   ET_TEXT2 = ERR2||' DL/I STATUS = ' || Q1_STAT;
   CALL ANALYZE_LOCAL_UIB;
   IF TEST_SW
     THEN DO;
       DIAG_TEXT = MODULE||' ghu dli stat '|| Q1_STAT;
       CALL WRITE_TO_OPER;
       IF SYSTEM_LIMITED
         THEN DO;
           DIAG_TEXT = MODULE||' SYSTEM IS LIMITED ';
           CALL WRITE_TO_OPER;
         END;
     END;

   IF SYSTEM_LIMITED
     THEN DO;
       RET_CODE = 12;
       REPLY_COMMENT = ' IMS/DBC PROBLEM AT HUB 8';
       CALL SEND_REPLY;
     END;

   SELECT (Q1_STAT);
     WHEN ('  ') ;
     OTHERWISE  DO;
       ET_TEXT2 = ERR2||' DL/I STATUS = ' || Q1_STAT;
       DLI_STAT = Q1_STAT;
       CALL WRITE_ERROR_MESSAGES;
       REPLY_COMMENT = ' GHU QSGENL PROBLEM AT HUB';
       CALL SEND_REPLY;
     END;
   END;                                /* END SELECT */

   GENL.QSARP_S = SUBSTR(IO_BUF,90,1);  /* SAVE CURRENT OFFLOAD FLAG */

   FUNC = 'DLET';
   IF TEST_SW
     THEN DO;
        DIAG_TEXT = MODULE||' ABOUT TO '||FUNC||' QSGENL SEGMENTS';
        CALL WRITE_TO_OPER;
     END;
   ET_TEXT1 = ERR2||MODULE||' UNABLE TO DELETE QSGENL SEGMENT '||
                                     'AT THE HUB QSAR DB';

   CALL PLITDLI (THREE,
                 FUNC,
                 QSAR1_PCB,
                 IO_BUF);

   ET_TEXT2 = ERR2||' DL/I STATUS = ' || Q1_STAT;
   CALL ANALYZE_LOCAL_UIB;

   IF SYSTEM_LIMITED
     THEN DO;
       RET_CODE = 12;
       REPLY_COMMENT = ' IMS/DBC PROBLEM AT HUB 9';
       CALL SEND_REPLY;
     END;

   SELECT(Q1_STAT) ;
     WHEN ('  ') ;
     OTHERWISE
       DO;
         ET_TEXT2 = ERR2||' DL/I STATUS = ' || Q1_STAT;
         DLI_STAT = Q1_STAT;
         CALL WRITE_ERROR_MESSAGES;
         REPLY_COMMENT = ' DELETE QSGENL PROBLEM AT HUB';
         CALL SEND_REPLY;
       END;
   END;                                  /* END SELECT                */

   FUNC = 'ISRT';
   IF TEST_SW
     THEN DO;
        DIAG_TEXT = MODULE||' ABOUT TO '||FUNC||' QSGENL SEGMENTS';
        CALL WRITE_TO_OPER;
     END;

   ET_TEXT1 = ERR2||MODULE||' UNABLE TO '||FUNC||' QSGENL SEGMENT '||
                                     'AT THE HUB QSAR DB 2';
   CALL PLITDLI (FOUR,
                 FUNC,
                 QSAR1_PCB,
                 GENL_BUF,
                 SSA_QSGENL_UNQUAL);

   ET_TEXT2 = ERR2||' DL/I STATUS = ' || Q1_STAT;
   CALL ANALYZE_LOCAL_UIB;

   IF SYSTEM_LIMITED
     THEN DO;
       RET_CODE = 12;
       REPLY_COMMENT = ' IMS/DBC PROBLEM AT HUB 10';
       CALL SEND_REPLY;
     END;

   SELECT (Q1_STAT);
     WHEN ('  ') ;
     OTHERWISE
       DO;
         ET_TEXT2 = ERR2||' DL/I STATUS = ' || Q1_STAT;
         DLI_STAT = Q1_STAT;
         CALL WRITE_ERROR_MESSAGES;
         REPLY_COMMENT = ' INSERT GENL PROB AT HUB AFTER DELETE';
         CALL SEND_REPLY;
        END;
   END;

 END DELETE_INS_QSGENL;

 %PAGE;
 CHECK_RESPONSE: PROC REORDER;
 /*********************************************************************/
 /*  CHECK FOR SUCCESSFUL CICS CALL ....   TAKE APPROPRIATE ACTION    */
 /*********************************************************************/
   IF RESPONSE ^= DFHRESP(NORMAL)
    THEN DO;
       PIC2 = RESPONSE;
       ET_TEXT2 = ERR2||MODULE||' BAD RESPONSE CODE = '||PIC2;
       SYSTEM_LIMITED = YES;
       CICS_STAT = PIC2;
       CALL WRITE_ERROR_MESSAGES;
    END;
    ELSE DO;
      CICS_STAT = '';
      PIC2      = 0;
    END;

 END CHECK_RESPONSE;

 %PAGE;
 ANALYZE_LOCAL_UIB: PROC REORDER;
 /*********************************************************************/
 /* CHECK FOR VALID RETURN CODE FROM DLI USER INTERFACE BLOCK         */
 /* BEFORE CHECKING THE PCB STATUS CODE.                              */
 /*********************************************************************/
   IF UIBFCTR = NULL
     THEN OKAY = '1'B;
     ELSE DO;
       OKAY = '0'B;
       RETCODE1 = UIBFCTR;
       RETCODE2 = UIBDLTR;
       ET_TEXT2 = ERR2||MODULE||' UIBFCTR='||RETCODE1||
                  ' UIBDLTR='||RETCODE2||' PSB='||PSBNAME||
                  ' FUNC='||FUNC;
       IF TEST_SW
         THEN DO;
           DIAG_TEXT = ET_TEXT2;
           CALL WRITE_TO_OPER;
         END;
       SYSTEM_LIMITED = YES;
       CALL WRITE_ERROR_MESSAGES;
     END;

 END ANALYZE_LOCAL_UIB;


 %PAGE;
 /*********************************************************************/
 /*********************************************************************/
 WRITE_ERROR_MESSAGES: PROC;

   IF ET_TEXT1 > ' '
     THEN EXEC CICS WRITE OPERATOR TEXT(ET_TEXT1);
   IF ET_TEXT2 > ' '
     THEN EXEC CICS WRITE OPERATOR TEXT(ET_TEXT2);
   IF ET_TEXT3 > ' '
     THEN EXEC CICS WRITE OPERATOR TEXT(ET_TEXT3);

   ET_TEXT1 = '';
   ET_TEXT2 = '';

 END WRITE_ERROR_MESSAGES;

 %PAGE;
 /*********************************************************************/
 /* AREA HAS OLD LEVEL OF CODE AND DATA BASE, THE HUB HAS THE NEW     */
 /* LEVEL OF CODE AND DATA BASE.                                      */
 /*                ??? TO ???.                                        */
 /*********************************************************************/
 CONVERT_AREA_DATA: PROC REORDER;

   IF TEST_SW
     THEN DO;
       DIAG_TEXT = MODULE||' CONVERT_AREA_DATA DOC '||
                               SUBSTR(GENL_BUF,1,9);
       CALL WRITE_TO_OPER;
     END;

 END CONVERT_AREA_DATA;

 %PAGE;
 /********************************************************************/
 /* UNCONVERT AREA DATA                                              */
 /* 7NSM QSGENL IS CONVERTING TO 730 BYTES AND QSINVT TO 275         */
 /* need to convert the NEW 730 back to the OLD 700 QSGENL before    */
 /* inserting into HUB QSAR DB.                                      */
 /********************************************************************/
 UNCONVERT_AREA_DATA: PROC REORDER;

   IF TEST_SW
     THEN DO;
       DIAG_TEXT       = MODULE||' UNCONVERT_AREA_DATA DOC '||
                               SUBSTR(GENL_BUF,1,9);
       CALL WRITE_TO_OPER;
     END;
   OLD_QSGENL = GENL, BY NAME;  /*  MOVE IT TO THE CONVERT BUF */

   GENL_BUF = '';

   GENL_BUF = OLD_GENL_BUF;
   
   OLD_QSINVT = INVT, BY NAME;  /*  MOVE IT TO THE CONVERT BUF */

   INVT_BUF = '';

   INVT_BUF = OLD_INVT_BUF;

 END UNCONVERT_AREA_DATA;

 %PAGE;
 /*********************************************************************/
 /*********************************************************************/
 SEND_REPLY: PROC REORDER;

   PIC2 = RESPONSE;

   IF TEST_SW THEN
     DO;
        DIAG_TEXT =MODULE||' MIRROR DLI STAT '||DLI_STAT||
                ' CICS RESP '||CICS_RESP||' RET CODE '||PIC2;
       CALL WRITE_TO_OPER;
     END;

   EXEC CICS SEND CONVID(CONVID)
             FROM (REPLY_STAT)
             LAST WAIT;

   IF RET_CODE = 16
     THEN SIGNAL ERROR;

   EXEC CICS RETURN;

 END SEND_REPLY;

 %PAGE;
 /*********************************************************************/
 /*                                                                   */
 /*********************************************************************/
 WRITE_TO_OPER: PROC;

   /* EXEC CICS WRITE OPERATOR TEXT(DIAG_TEXT)  */
  EXEC CICS WRITEQ TD QUEUE('CESE') FROM(DIAG_TEXT) LENGTH(80);

 END WRITE_TO_OPER;


