 %PAGE;
 /*****************************************************************/
 /*   ON CONVERSION                                               */
 /*****************************************************************/

 ON CONVERSION
   BEGIN;
     ET_DUMPCODE = 'Y';
     ET_TEXT1 = ERR1||MODULE||' HAD CONVERSION ERROR '|| GENL.S0IDOCMN;
     ET_TEXT2 = ERR1||MODULE||' ONSOURCE '||ONSOURCE||'LOC '||ONLOC;
     RET_CODE = 16;
     REPLY_COMMENT = ' CONVERSION ERROR ON DOC '|| GENL.S0IDOCMN;
   END;

 /*****************************************************************/
 /*   ON ERROR                                                    */
 /*****************************************************************/

   ON ERROR
      BEGIN;
        ON ERROR SYSTEM;

        FUNC = 'TERM';                     /* COMMIT ANY CHANGES */
        CALL PLITDLI (ONE,FUNC);

        WTO_FLAG = 'Y';

        ET_PROGRAM_ID = 'AEQMI2R';
        ET_EIB = EIB_STRING;

        EXEC CICS XCTL PROGRAM('AEQERRP')
             COMMAREA(ERROR_DATA)
             RESP(RESPONSE);

        IF RESPONSE ^= DFHRESP(NORMAL) THEN
          NOT_NORMAL: DO;
              EXEC CICS DUMP DUMPCODE('MI2R') TASK;
              EXEC CICS RETURN;
          END NOT_NORMAL;
      END;

 %PAGE;
 /*********************************************************************/
 /* ----------------------------------------------------------------- */
 /*         START MAIN PROCESSING                                     */
 /* ----------------------------------------------------------------- */
 /*********************************************************************/

 EXEC CICS ADDRESS CWA(CWA_PTR) RESP(RESPONSE);
 CALL CHECK_RESPONSE;

 IF SYSTEM_LIMITED
   THEN DO;
     REPLY_COMMENT = ' ADDRESSING CWA FAILURE';
     CALL SEND_REPLY;
   END;

 CONVID = EIBTRMID;                      /* GET CONVERSATION ID FROM  */
                                         /* EIB                       */
 MIRROR_BUF = '';

 EXEC CICS RECEIVE CONVID (CONVID)
                   INTO (MIRROR_BUF)
                   LENGTH(CSTG(MIRROR_BUF))
                   RESP(RESPONSE);

 /*  CALL CHECK_RESPONSE  do not do this (lbs) memo to myself */
 IF SYSTEM_LIMITED &
    RESPONSE ^= DFHRESP(EOC) &           /* END OF CHAIN RESPONSE     */
    RESPONSE ^= DFHRESP(NORMAL)          /* NORMAL RESPONSE           */
   THEN DO;
     REPLY_COMMENT = ' RECEIVE DATA INTO BUFFER PROBLEM';
     CALL SEND_REPLY;
   END;
   
   I = 01;                               /* RCQ1882,  Area always 01  */
                                         /* ------------------------- */
 /* PIC2 = CWAARNUM; RCQ1882 */          /* GET AREA NUMBER           */
                                         /* ------------------------- */
 /* RCQ1882 : Area always 01, so remove below if ...else              */
 /*IF PIC2 > 10
   THEN I = PIC2 - 10;     i.e. area 11 check flag 1, area 12 check  */
 /*ELSE I = PIC2;          flag 2, this could happen in test only     */

 /* ----------------------------------------------------------------- */
 /* CALL THE CORRECT PROCEDURE TO CONVERT OR UN-CONVERT THE QSAR DATA */
 /* SENT FROM THE AREA.  THIS DEPENDS ON WHAT CICS REGIONS HAVE BEEN  */
 /* CONVERTED.                                                        */
 /* ----------------------------------------------------------------- */
 IF CWA_QSAR_TRACE = 'M' | 
    CWA_QSAR_TRACE = 'Y'
   THEN DO;
     TEST_SW = '1'B;
     DIAG_TEXT = MODULE||' AREA '   ||I||' '||CWA_AREA_CONV(I)||
                         ' HUBCNVT '||I||' '||CWA_QHUB_CONV(I);
     CALL WRITE_TO_OPER;
   END;

 SELECT ;
          /* BOTH AT NEW LEVEL DO NOTHING                           */
   WHEN (CWA_QHUB_CONV(I) = 'Y' & CWA_AREA_CONV(I) = 'Y');

          /* HUB AT OLD LEVEL AREA AT OLD LEVEL NO CONVERSION NEEDED*/
   WHEN (CWA_QHUB_CONV(I) = 'N' & CWA_AREA_CONV(I) = 'N');

          /* HUB AT OLD LEVEL AREA AT NEW LEVEL UNCONVERT AREA DATA */
   WHEN (CWA_QHUB_CONV(I) = 'N' & CWA_AREA_CONV(I) = 'Y')
         DO;
           IF TEST_SW
             THEN DO;
               DIAG_TEXT = MODULE||' HUB NOT CONVERTED YET, DOC '||
                                SUBSTR(GENL_BUF,1,9);
               CALL WRITE_TO_OPER;
             END;

           CALL UNCONVERT_AREA_DATA ;
         END;

          /* HUB AT NEW LEVEL AREA AT OLD LEVEL CONVERT AREA DATA   */
   WHEN (CWA_QHUB_CONV(I) = 'Y' & CWA_AREA_CONV(I) = 'N')
     DO;
       CALL CONVERT_AREA_DATA;
     END;


   OTHERWISE
     DO;
       ET_TEXT1 = ERR2||MODULE||' CONVERT FLAGS NOT SET CORRECTLY HUB ='
                    ||CWA_QHUB_CONV(I)||
                   'AREA ='||PIC2||' '||CWA_AREA_CONV(I);
       EXEC CICS WRITE OPERATOR TEXT(ET_TEXT1);
       RET_CODE = 12;
       REPLY_COMMENT = ' CONVERT FLAG PROBLEM HUB ='||CWA_QHUB_CONV(I)||
                       ' AREA ='||CWA_AREA_CONV(I);
       CALL SEND_REPLY;
     END;
 END;

 CALL INSERT_SEGMENTS;

 CALL SEND_REPLY;


 %PAGE;
 INSERT_SEGMENTS: PROC REORDER;
 /*********************************************************************/
 /*  SCHEDULE PSB - AEQMI2R                                           */
 /*  IF FAIL, RETURN (SYSTEM LIMITED FLAG IS SET)                     */
 /*    ISRT QSGENL                                                    */
 /*    IF II RETURN CODE, DELETE THE CURRENT QSGENL SEG AND           */
 /*      INSERT THE CURRENT QSGENL SEGMENT,                           */
 /*      KEEP GOING FOR DEPENDENTS                                    */
 /*    IF PARTS,                                                      */
 /*      ISRT PARTS SEGMENT                                           */
 /*      IF II RETURN CODE, KEEP GOING FOR OTHER DEPENDENTS           */
 /*    IF INVT,                                                       */
 /*      ISRT INVT SEGMENT                                            */
 /*      IF II RETURN CODE, KEEP GOING FOR OTHER DEPENDENTS           */
 /*    IF NARR,                                                       */
 /*      ISRT NARR SEGMENT                                            */
 /*      IF II RETURN CODE, KEEP GOING FOR OTHER DEPENDENTS           */
 /*    IF MISC,                                                       */
 /*      ISRT MISC SEGMENT                                            */
 /*      IF II RETURN CODE, KEEP GOING FOR OTHER DEPENDENTS           */
 /*    IF QSAR BILLING QBBILL SEGMENT                                 */
 /*      ISRT BILLING SEGMENT                                         */
 /*      IF II RETURN CODE, KEEP GOING FOR OTHER DEPENDENTS           */
 /*    IF QSAR BILLING PARTS SEGMENT                                  */
 /*      ISRT QSAR BILLING PARTS                                      */
 /*      IF II RETURN CODE, KEEP GOING FOR OTHER DEPENDENTS           */
 /*    IF QSAR BILLING NARRATIVE                                      */
 /*      ISRT QSAR BILLING NARRATIVE SEGMENT                          */
 /*      IF II RETURN CODE, KEEP GOING FOR OTHER DEPENDENTS           */
 /*                                                                   */
 /*  TERMINATE AEQMI2R PSB                                            */
 /*                                                                   */
 /*********************************************************************/
   SYSTEM_LIMITED = NO;                  /* ASSUME SYSTEM GOOD HEALTH */

   IF TEST_SW
     THEN DO;
       DIAG_TEXT = MODULE||' INSIDE INSERT SEGMENTS ';
       CALL WRITE_TO_OPER;
     END;
     
   CALL TERMINATE_PSB;
   FUNC    = 'PCB ';                     /* SCHEDULE AEQMI2R PSB      */
   PSBNAME = 'AEQMI2R ';
   IF TEST_SW
     THEN DO;
        DIAG_TEXT = MODULE||' ABOUT TO SCHEDULE '||FUNC||' '|| PSBNAME;
        CALL WRITE_TO_OPER;
     END;

   ET_TEXT1 = ERR2||MODULE||' UNABLE TO SCHEDULE '||PSBNAME;

   CALL PLITDLI (THREE,FUNC,PSBNAME,UIBPTR);

   CALL ANALYZE_LOCAL_UIB;
   IF SYSTEM_LIMITED
     THEN DO;
       RET_CODE = 12;                    /* INDICATE A MIRRORING PROB */
       REPLY_COMMENT = ' CAN NOT SCHEDULE AEQMI2R AT HUB';
       CALL SEND_REPLY;
     END;

   ET_TEXT1 = ERR1||MODULE||' UNABLE TO EXEC CICS ASKTIME';

   EXEC CICS ASKTIME ABSTIME(UTIME) RESP(RESPONSE);

   CALL CHECK_RESPONSE;
   IF SYSTEM_LIMITED
     THEN DO;
       REPLY_COMMENT = ' CICS ASKTIME PROBLEM AT HUB';
       CALL SEND_REPLY;
     END;

   ET_TEXT1 = ERR1||MODULE||' UNABLE TO EXEC CICS FORMATTIME';
   EXEC CICS FORMATTIME ABSTIME(UTIME)
                        YYMMDD(DATE_YY_MM_DD)
                        TIME(TIME_HH_MM_SS)
                        RESP(RESPONSE);

   CALL CHECK_RESPONSE;
   IF SYSTEM_LIMITED
     THEN DO;
       REPLY_COMMENT = ' CICS FORMATTIME PROBLEM AT HUB';
       CALL SEND_REPLY;
     END;

   QSGENL_KEYVAL = GENL.S0IDOCMN;
   ET_TEXT3 = ERR1||MODULE||' DOC NUM '||GENL.S0IDOCMN;
                                         /* MIRROR TIME               */
   GENL.CGIERRMS(2) = 997;
 /*SUBSTR(GENL_BUF,LENGTH(GENL_BUF)-10,10) =           */
 /*     SUBSTR(DATE_YYMMDD,1,6)||SUBSTR(TIME_HHMM,1,4) */

   FUNC = 'ISRT';

   IF TEST_SW
     THEN DO;
       DIAG_TEXT = MODULE||' ABOUT TO '||FUNC||' QSGENL SEGMENTS'||
                           ' DOC # = ('||GENL.S0IDOCMN||')';
       CALL WRITE_TO_OPER;
     END;

   ET_TEXT1 = ERR2||MODULE||' UNABLE TO INSERT QSGENL SEGMENT '||
                                         'AT THE HUB DB 1';

   CALL PLITDLI (FOUR,
                    FUNC,
                    QSAR1_PCB,
                    GENL_BUF,
                    SSA_QSGENL_UNQUAL);

   CALL ANALYZE_LOCAL_UIB;

   IF ^SYSTEM_LIMITED
     THEN SELECT (Q1_STAT);
            WHEN ('  ') ;
            WHEN ('II')  CALL DELETE_INS_QSGENL;  /* DUPE QSAR ROOT   */
            OTHERWISE
              DO;                        /* BAD DLI STATUS CODE       */
                ET_TEXT2 = ERR2||' DL/I STATUS = ' || Q1_STAT;
                DLI_STAT = Q1_STAT;
                CALL WRITE_ERROR_MESSAGES;
                REPLY_COMMENT = ' INSERT QSGENL PROBLEM AT HUB';
                CALL SEND_REPLY;
             END;
           END;                          /* END OF SELECT             */
      ELSE DO;
        RET_CODE = 12;
        REPLY_COMMENT = ' IMS/DBC PROBLEM AT HUB 1';
        CALL SEND_REPLY;
      END;

 /* ---------------------------------------------------------------- */
 /* INSERT PARTS IF PRESENT                                          */
 /* ---------------------------------------------------------------- */
                                        /* INSERT PART 1             */
   FUNC = 'ISRT';
   DO I = 1 TO 4;
     IF ^SYSTEM_LIMITED & PART_BUF(I) ^= ' '
       THEN DO;
         PIC2 = I;
         ET_TEXT1 = ERR2||MODULE||' UNABLE TO INSERT'||
                           ' QSPART'||PIC2||' SEGMENT AT THE HUB DB';
         IF TEST_SW
           THEN DO;
             DIAG_TEXT = MODULE||' ABOUT TO '||FUNC||' QSPART '||
                                 PIC2||' SEGMENTS';
             CALL WRITE_TO_OPER;
            END;

         CALL PLITDLI (FIVE,
                       FUNC,
                       QSAR1_PCB,
                       PART_BUF(I),
                       SSA_QSGENL_QUAL,
                       SSA_QSPART_UNQUAL);

         CALL ANALYZE_LOCAL_UIB;

         IF ^SYSTEM_LIMITED  &
             Q1_STAT ^= '  ' &
             Q1_STAT ^= 'II'
           THEN DO;                     /* BAD DLI STATUS CODE       */
             ET_TEXT2 = ERR2||' DL/I STATUS = ' || Q1_STAT;
             DLI_STAT = Q1_STAT;
             CALL WRITE_ERROR_MESSAGES;
             REPLY_COMMENT = ' INSERT PART PROBLEM AT HUB';
             CALL SEND_REPLY;
           END;
         IF SYSTEM_LIMITED
           THEN DO;
             RET_CODE = 12;
             REPLY_COMMENT = ' IMS/DBC PROBLEM AT HUB 2';
             CALL SEND_REPLY;
           END;
       END;
   END;                                 /* END OF DO I = 1 TO 4      */

 /* ---------------------------------------------------------------- */
 /* INSERT QSINVT IF PRESENT                                         */
 /* ---------------------------------------------------------------- */
   IF ^SYSTEM_LIMITED & INVT_BUF ^= ' '
     THEN DO;
       ET_TEXT1 = ERR2||MODULE||' UNABLE TO '||FUNC||
                          ' QSINVT SEGMENT AT THE HUB DB';
       IF TEST_SW
         THEN DO;
           DIAG_TEXT = MODULE||' ABOUT TO '||FUNC||' INVT SEGMENTS';
           CALL WRITE_TO_OPER;
         END;

       CALL PLITDLI (FIVE,
                     FUNC,
                     QSAR1_PCB,
                     INVT_BUF,
                     SSA_QSGENL_QUAL,
                     SSA_QSINVT_UNQUAL);
       CALL ANALYZE_LOCAL_UIB;

       IF ^SYSTEM_LIMITED   &
           Q1_STAT ^= '  '  &
           Q1_STAT ^= 'II'
         THEN DO;                        /* BAD DLI STATUS CODE       */
           ET_TEXT2 = ERR2||' DL/I STATUS = ' || Q1_STAT;
           DLI_STAT = Q1_STAT;
           CALL WRITE_ERROR_MESSAGES;
           REPLY_COMMENT = ' INSERT INVENTORY PROBLEM AT HUB';
           CALL SEND_REPLY;
         END;

       IF SYSTEM_LIMITED
         THEN DO;
           RET_CODE = 12;
           REPLY_COMMENT = ' IMS/DBC PROBLEM AT HUB 3';
           CALL SEND_REPLY;
         END;
     END;

 /* ---------------------------------------------------------------- */
 /* INSERT QSNARR IF PRESENT                                         */
 /* ---------------------------------------------------------------- */
   IF ^SYSTEM_LIMITED & NARR_BUF ^= ' '
     THEN DO;
       ET_TEXT1 = ERR2||MODULE||' UNABLE TO '||FUNC||
                        'QSNARR SEGMENT AT THE HUB DB';
       IF TEST_SW
         THEN DO;
           DIAG_TEXT = MODULE||' ABOUT TO '||FUNC||' NARR SEGMENTS';
           CALL WRITE_TO_OPER;
         END;

       CALL PLITDLI (FIVE,
                     FUNC,
                     QSAR1_PCB,
                     NARR_BUF,
                     SSA_QSGENL_QUAL,
                     SSA_QSNARR_UNQUAL);
       CALL ANALYZE_LOCAL_UIB;

       IF ^SYSTEM_LIMITED &
           Q1_STAT ^= '  ' &
           Q1_STAT ^= 'II'
         THEN DO;                       /* BAD DLI STATUS CODE       */
           ET_TEXT2 = ERR2||' DL/I STATUS = ' || Q1_STAT;
           DLI_STAT = Q1_STAT;
           CALL WRITE_ERROR_MESSAGES;
           REPLY_COMMENT = ' INSERT NARRATIVE PROBLEM AT HUB';
           CALL SEND_REPLY;
         END;

       IF SYSTEM_LIMITED
         THEN DO;
           RET_CODE = 12;
           REPLY_COMMENT = ' IMS/DBC PROBLEM AT HUB 4';
           CALL SEND_REPLY;
         END;
     END;

 /* ---------------------------------------------------------------- */
 /* INSERT QSMISC IF PRESENT AND INSERT_MISC IS '1'B                 */
 /* ---------------------------------------------------------------- */
   IF ^SYSTEM_LIMITED & MISC_BUF ^= ' '
     THEN DO;
       ET_TEXT1 = ERR2||MODULE||' UNABLE TO '||FUNC||
                        'QSMISC SEGMENT AT THE HUB DB';
       IF TEST_SW
         THEN DO;
           DIAG_TEXT = MODULE||' ABOUT TO '||FUNC||' MISC SEGMENT';
           CALL WRITE_TO_OPER;
         END;

       CALL PLITDLI (FIVE,
                     FUNC,
                     QSAR1_PCB,
                     MISC_BUF,
                     SSA_QSGENL_QUAL,
                     SSA_QSMISC_UNQUAL);
       CALL ANALYZE_LOCAL_UIB;

       IF ^SYSTEM_LIMITED  &
           Q1_STAT ^= '  ' &
           Q1_STAT ^= 'II'
         THEN DO;                        /* BAD DLI STATUS CODE       */
           ET_TEXT2 = ERR2||' DL/I STATUS = ' || Q1_STAT;
           DLI_STAT = Q1_STAT;
           CALL WRITE_ERROR_MESSAGES;
           REPLY_COMMENT = ' INSERT MISC PROBLEM AT HUB';
           CALL SEND_REPLY;
         END;

       IF SYSTEM_LIMITED
         THEN DO;
           RET_CODE = 12;
           REPLY_COMMENT = ' IMS/DBC PROBLEM AT HUB 4';
           CALL SEND_REPLY;
         END;
     END;

 /* ---------------------------------------------------------------- */
 /* INSERT QBBILL IF PRESENT                                         */
 /* ---------------------------------------------------------------- */

   IF ^SYSTEM_LIMITED & QBBILL_BUF ^= ' '
     THEN DO;
       ET_TEXT1 = ERR2||MODULE||' UNABLE TO '||FUNC||
                          ' QBBILL SEGMENT AT THE HUB DB';
       IF TEST_SW
         THEN DO;
           DIAG_TEXT = MODULE||' ABOUT TO '||FUNC||' QBBILL SEGMENTS';
           CALL WRITE_TO_OPER;
         END;

       CALL PLITDLI (FIVE,
                     FUNC,
                     QSAR1_PCB,
                     QBBILL_BUF,
                     SSA_QSGENL_QUAL,
                     SSA_QBBILL_UNQUAL);

       CALL ANALYZE_LOCAL_UIB;

       IF ^SYSTEM_LIMITED  &
           Q1_STAT ^= '  ' &
           Q1_STAT ^= 'II'
         THEN DO;                        /* BAD DLI STATUS CODE       */
           ET_TEXT2 = ERR2||' DL/I STATUS = ' || Q1_STAT;
           DLI_STAT = Q1_STAT;
           CALL WRITE_ERROR_MESSAGES;
           REPLY_COMMENT = ' INSERT BILL PROBLEM AT HUB '||
                              SUBSTR(QBBILL_BUF,1,7);
           CALL SEND_REPLY;
         END;

       IF SYSTEM_LIMITED
         THEN DO;
           RET_CODE = 12;
           REPLY_COMMENT = ' IMS/DBC PROBLEM AT HUB 5';
           CALL SEND_REPLY;
         END;

     END;

 /* ---------------------------------------------------------------- */
 /* INSERT HSI BILLING PARTS IF PRESENT                              */
 /* ---------------------------------------------------------------- */

   DO K = 1 TO QBPART_CT;
     IF ^SYSTEM_LIMITED & QBPART_BUF(K) ^= '  '
       THEN DO;
         ET_TEXT1 = ERR2||MODULE||' UNABLE TO '||FUNC||
                        ' QBPART'||K||' SEGMENT'||' AT THE HUB DB';
       IF TEST_SW
         THEN DO;
           DIAG_TEXT = MODULE||' ABOUT TO '||FUNC||' QBPART SEGMENTS';
           CALL WRITE_TO_OPER;
         END;

         CALL PLITDLI (FIVE,
                       FUNC,
                       QSAR1_PCB,
                       QBPART_BUF(K),
                       SSA_QSGENL_QUAL,
                       SSA_QBPART_UNQUAL);

         CALL ANALYZE_LOCAL_UIB;

         IF ^SYSTEM_LIMITED  &
             Q1_STAT ^= '  ' &
             Q1_STAT ^= 'II'
           THEN DO;                      /* BAD DLI STATUS CODE       */
             ET_TEXT2 = ERR2||' DL/I STATUS = ' || Q1_STAT;
             DLI_STAT = Q1_STAT;
             CALL WRITE_ERROR_MESSAGES;
             REPLY_COMMENT = ' INSERT BILL PART PROBLEM AT HUB';
             CALL SEND_REPLY;
           END;

         IF SYSTEM_LIMITED
           THEN DO;
             RET_CODE = 12;
             REPLY_COMMENT = ' IMS/DBC PROBLEM AT HUB 6';
             CALL SEND_REPLY;
           END;
       END;

   END;

 /* ---------------------------------------------------------------- */
 /* INSERT QBNARR IF PRESENT                                         */
 /* ---------------------------------------------------------------- */
   IF ^SYSTEM_LIMITED & QBNARR_BUF ^= ''
     THEN DO;
       ET_TEXT1 = ERR2||MODULE||' UNABLE TO '||FUNC||' QBNARR SEGMENT'
                                     ||' AT THE HUB DB';
       IF TEST_SW
         THEN DO;
           DIAG_TEXT = MODULE||' ABOUT TO '||FUNC||' QBNARR SEGMENTS';
           CALL WRITE_TO_OPER;
         END;

       CALL PLITDLI (FIVE,
                     FUNC,
                     QSAR1_PCB,
                     QBNARR_BUF,
                     SSA_QSGENL_QUAL,
                     SSA_QBNARR_UNQUAL);

       CALL ANALYZE_LOCAL_UIB;

       IF ^SYSTEM_LIMITED &
            Q1_STAT ^= '  ' &
              Q1_STAT ^= 'II'
         THEN DO;                        /* BAD DLI STATUS CODE       */
           ET_TEXT2 = ERR2||' DL/I STATUS = ' || Q1_STAT;
           DLI_STAT = Q1_STAT;
           CALL WRITE_ERROR_MESSAGES;
           REPLY_COMMENT = ' INSERT BILL NARRATIVE PROBLEM AT HUB';
           CALL SEND_REPLY;
         END;

       IF SYSTEM_LIMITED
         THEN DO;
           RET_CODE = 12;
           REPLY_COMMENT = ' IMS/DBC PROBLEM AT HUB 7';
           CALL SEND_REPLY;
         END;
     END;
     
    CALL TERMINATE_PSB;
  /* FUNC = 'TERM';                      TERMINATE AEQMI2R PSB     */
