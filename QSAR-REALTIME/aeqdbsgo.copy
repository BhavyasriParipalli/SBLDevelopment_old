       5 XMCOMPIL FIXED BIN(15),/*                          611- 612 */
       5 XMTABID  FIXED BIN(15),/*                          613- 614 */
       5 XMMSGFLG,              /*                                   */
         7 XMPTRAMS    BIT(1),  /*                          615- 615 */
         7 XMHUB       BIT(1),  /*                          615- 615 */
         7 XMIPRPSR    BIT(1),  /*                          615- 615 */
         7 XMNUS12     BIT(5),  /*                          615- 615 */
