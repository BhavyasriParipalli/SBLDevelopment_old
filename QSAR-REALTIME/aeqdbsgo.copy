 /*                                                                  */
 /* 05/08/96 S9570033  6NSG  ASSET TRACKING / ADD PRF PROMPT         */
 /*                          TO PT TABLE                             */
 /*                          SPLIT ECA, MES AND REA OUT OF           */
 /*                          MICROCODE FIELD.                        */
 /*           S9600243       REMOVE ROLM PRODUCTS FROM TAC           */
 /*                          REPORTING. ALSO REMOVE CAUSE REPAIR     */
 /*                          AND THE SEVERITY PROMPTS.               */
 /* 09/11/96 S9570033  6NSJ  ADD TIN TO PARTS DATA              LBS  */
 /* 10/18/96 S9600620  6NSP  ADD SEVERITY PROMPT TO COMMON      MKP  */
 /* 05/05/98 I0720655  8NSE  SWITCH ECA & REA FIELDS            LBS  */
 /* 09/16/99 S99093    9NSA  ADD WAYBILL NUMBER                 MKP  */
 /* 05/15/01 S01032    1NSP  add  XDCWKNAC                      LBS  */
 /* 02/25/03 CICS TS13 3NSG  add FIMSABND,FREQUEUE              LBS  */
 /*                                                                  */
 /********************************************************************/
 /********************************************************************/
 /* QSAR ROOT SEGMENT. HERE, ALL VARIABLES BEGIN WITH "QDR".         */
 /********************************************************************/
     3 XDROOT ,
       5 XDRKEY,                /* KEY FIELD                         */
         7 XDREMPSR CHAR(6),    /* EMPLOYEE SERIAL      OFF 001- 006 */
         7 XDRDAYWK CHAR(1),    /* DAY OF WEEK (1=SATURDAY) 007- 007 */
         7 XDRAMPM  CHAR(1),    /* A=A.M. (STOP TIME  0.0-1 008- 008 */
                                /* P=P.M. (STOP TIME 14.0-24.0)      */
 /********************************************************************/
 /* QSAR DATE SEGMENT. HERE, ALL VARIABLES BEGIN WITH "QDD".         */
 /********************************************************************/
     3 XDDATE,
       5 XDDRECDT CHAR(8),      /* KEY: DATE (YYMMDD)   OFF 009- 016 */
       5 XDDPTLEV CHAR(2),      /* PT LEVEL FOR LAYOUT CHAN 017- 018 */
 /********************************************************************/
 /* QSAR COMMON SEGMENT. HERE, ALL VARIABLES BEGIN WITH "XDC".       */
 /********************************************************************/
     3 XDCOMMON,
       5 XDCKEY,                /* KEY FIELD                         */
         7 XDCSTOP  CHAR(3),    /* STOP TIME (HHT)      OFF 019- 021 */
         7 XDCRECNO CHAR(9),    /* RECORD NO GENERATED BY P 022- 030 */
       5 XDCSEQCL CHAR(3),      /* SEQUENCE/CALL NUMBER     031- 033 */
       5 XDCOMPL  CHAR(4),      /* PT REC COMPLETION (HHMM) 034- 037 */
       5 XDCRCVTM CHAR(4),      /* HOST RECEIVE TIME (HHMM) 038- 041 */
       5 XDCRCVDT CHAR(4),      /* HOST RECEIVE DATE (MMDD) 042- 045 */
       5 XDCSC    CHAR(2),      /* SERVICE CODE             046- 047 */
       5 XDCACTUL CHAR(3),      /* ACTUAL HOURS (HHT)       048- 050 */
       5 XDCTRAVL CHAR(3),      /* TRAVEL HOURS (HHT)       051- 053 */
       5 XDCMILES CHAR(3),      /* MILEAGE-ACTUAL MILES     054- 056 */
       5 XDCPAD01 CHAR(1),      /* PAD FOR FUTURE EXPANSION 057- 057 */
       5 XDCEXPNS CHAR(6),      /* EXPENSE                  058- 063 */
       5 XDCOVRTM CHAR(3),      /* OVERTIME HOURS (HHT)     064- 066 */
       5 XDCMTYPE CHAR(4),      /* MACHINE TYPE             067- 070 */
       5 XDCMSERL CHAR(22),     /* MACHINE SERIAL           071- 092 */
       5 XDCSREQ  CHAR(1),      /* SCHEDULED (SERVICE REQUE 093- 093 */
       5 XDCIAP   CHAR(1),      /* COMPLETION CODE (CIDASP) 094- 094 */
       5 XDCTAC   CHAR(3),      /* TECHNICAL AREA CODE (IBM 095- 097 */
                                /* BILL TYPE           (SC 94)       */
       5 XDCPAD04 CHAR(1),      /* PAD                      098- 098 */
       5 XDCACTN  CHAR(2),      /* ACTION CODE         (IBM 099- 100 */
       5 XDCACTIV CHAR(2),      /* ACTIVITY CODE            101- 102 */
       5 XDCOTHMK CHAR(3),      /* OTHER/MARKETING B/O      103- 105 */
       5 XDCPREXP CHAR(4),      /* COMMON PROB # EXPANSION  106- 109 */
       5 XDCPROB  CHAR(7),      /* CE ENTERED PROBLEM NUMBE 110- 116 */
       5 XDCBILSP CHAR(1),      /* BILLABLE/SPECIAL ACTIVIT 117- 117 */
       5 XDCTBLVL FIXED BIN(15),/* PT TBL VERSION AND LEVEL 118- 119 */
                                /* NOT ASSIGNED DIRECTLY FROM PT     */
       5 XDCSTYPE CHAR(1),      /* CONCURRENT(SERVICE TYPE) 120- 120 */
       5 XDCFLAGS,              /* BIT FLAGS                         */
         7 XDCRCORG BIT(1),     /*   0 = FROM PT     1 = FR 121- 121 */
         7 XDCPSFLG BIT(1),     /*   0 = IPAR/PARTS  1 = PS 121- 121 */
         7 XDCPRFLG BIT(1),     /*   0 = IPAR/PSAR   1 = PA 121- 121 */
         7 XDCPTFLG BIT(1),     /*   0 = IPAR/PSAR   1 = PA 121- 121 */
         7 XDCPAD02 BIT(4),     /* PAD                      121- 121 */
       5 XDCSHIFT CHAR(1),      /* ASSIGNED SHIFT           122- 122 */
       5 XDCPROB2 CHAR(7),      /* ORIGINAL CAD ASSIGNED PR 123- 129 */
       5 XDCPMHRS CHAR(3),      /* PM HOURS                 130- 132 */
       5 XDCSVRTY CHAR(1),      /* SEVERITY                 133- 133 */
       5 XDCWKNAC CHAR(6),      /* Work Number Activity ID  134- 139 */
       5 XDCUSTPR CHAR(15),     /* Customer Problem Number  140- 154 */
                                /* (LEXMARK CLAIM NUMBER)            */
       5 XDCPAD03 CHAR(24),     /* PAD                      155- 178 */
